{% extends 'layouts/page.njk' %}

{% set displayBreadcrumbs = false %}
{% set title = "We could not find" + " " + data['q'] %}

{% block content %}
<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds-from-desktop">
    <h1 class="govuk-heading-l">{{notFoundUrl.heading}} {{msError}}</h1>
    <p class="govuk-body"><a href="/">{{notFoundUrl.paragraphs.a}}</a></p>
  </div>
</div>
{% endblock %}
