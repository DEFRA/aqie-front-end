{% extends 'layouts/page.njk' %}

{% set displayBreadcrumbs = false %}
{% set title = "We could not find" + " " + data['q'] %}

{% block content %}
<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

      <h1 class="govuk-heading-l">{{notFoundUrl.heading}}</h1>
      <p class="govuk-body">
        {{notFoundUrl.paragraphs.a}}<strong>{{url}}</strong>
      </p>
      <p class="govuk-body">{{notFoundUrl.paragraphs.b}}</p>
      <ul class="govuk-list govuk-list--bullet">
        <li>{{notFoundUrl.paragraphs.c}}</li>
      </ul>
    </div>
  </div>
{% endblock %}
